.hidden {
	visibility: hidden;
}

.map .item {
	position: absolute;
	background-color: #ffc;
	color: #777;
	// width: 10%;
	// height: 0;
	// padding-bottom: 10%;
	// padding-top: 4px;
	// padding-left: 4px;
	// padding-right: 4px;
	width: 70px;
	height: 70px;
	padding: 4px;
	font-family: "Reenie Beanie",arial,sans-serif;
	box-shadow: 5px 5px 7px rgba(33,33,33,.7);
	transform: rotate(-6deg);
}

.map .item:nth-child(even) {
	transform: rotate(4deg);
	background: #cfc;
/*  -o-transform:rotate(4deg);
